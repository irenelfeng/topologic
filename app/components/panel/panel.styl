#panel
  width 20%
  float left
  background-color lighten(#29C8E1, 90%)
  border-right 1px solid #29C8E1

  #panel-header-container
    height 30px
    width 100%
    padding-bottom 5px
    flexbox(flex)
    justify-content(space-between)

    #panel-header
      padding-top 10px
      font-size 1.3em
      text-transform capitalize

    #panel-plus
      padding-top 10px
      padding-right 10px
      height 20px
      cursor pointer
      

