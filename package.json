{
  "name": "topologic",
  "description": "Description",
  "author": "CS89_Group",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-sidebar": "^2.1.1",
    "reactify": "^1.1.1"
  },
  "browserify": {
    "extension": [
      "jsx"
    ],
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "watchify": {
    "extension": [
      "jsx"
    ],
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "build": "browserify -t [ babelify --presets [ es2015 react ] ] app/main.js -o bundle.js && stylus app/style.styl -o style.css",
    "watch": "watchify -t [ babelify --presets [ es2015 react ] ] app/main.js -o bundle.js",
    "bw": "npm run build && npm run watch"
  }
}
